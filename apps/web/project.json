{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
        "color": true,
        "command": "npm run dev"
      }
    },
    "dev:uat": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/web",
