    }
  },
  "build": {
    "executor": "nx:run-commands",
    "options": {
      "cwd": "apps/web",
      "color": true,
      "command": "npm run build"
    }
  },
  "build:uat": {
    "executor": "nx:run-commands",
    "options": {
      "cwd": "apps/web",
      "color": true,
      "command": "npm run build:uat"
    }
  }
}
